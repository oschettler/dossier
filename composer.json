{
    "name": "oschettler/dossier",
    "type": "library",
    "description": "Generate dossier websites from a list of project repositories",
    "homepage": "https://github.com/oschettler/dossier",
    "license": "MIT",
    "authors": [
        {
            "name": "See contributors",
            "homepage": "https://github.com/oschettler/dossier/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/oschettler/dossier/issues"
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "pdepend/pdepend": "~1.0",
        "phploc/phploc": "~2.0",
        "phpmd/phpmd": "~1.0",
        "phpunit/phpunit": "~3.0",
        "scrutinizer/ocular": "~1.0",
        "sebastian/phpcpd": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Dossier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dossier\\Test\\": "test/"
        }
    }
}
